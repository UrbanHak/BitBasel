{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "@next/next/no-img-element": "off",
    "react-hooks/exhaustive-deps": "warn",
    "no-unused-vars": "warn",
    "react/no-unescaped-entities": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
